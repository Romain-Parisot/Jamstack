{
  "kind": "collectionType",
  "collectionName": "markets",
  "info": {
    "singularName": "market",
    "pluralName": "markets",
    "displayName": "Market",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ticker": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "price": {
      "type": "float"
    },
    "marcketcap": {
      "type": "biginteger"
    },
    "shares": {
      "type": "float"
    },
    "name": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "favorites": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "markets_favorites"
    }
  }
}
